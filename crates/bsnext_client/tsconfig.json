{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "noImplicitAny": false
  },
  "include": [
    "inject",
    "ui"
  ],
  "exclude": [
    "ui/dist",
    "inject/dist"
  ]
}
